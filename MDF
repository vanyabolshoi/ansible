- name: Показать только имена файлов чисто
  hosts: windows
  gather_facts: no

  tasks:
    - name: Найти файлы .mdf в C:\DataSQL
      win_find:
        paths: C:\DataSQL
        patterns: "*.mdf"
      register: mdf_files

    - name: Вывести имена файлов без лишней информации
      ansible.builtin.debug:
        msg: "{{ item.filename }}"
      loop: "{{ mdf_files.files }}"
      loop_control:
        label: "{{ item.filename }}"
